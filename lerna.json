{
    "packages": [
        "packages/*"
    ],
    "version": "3.5.2",
    "command": {
        "version": {
            "conventionalCommits": true,
            "createRelease": "github",
            "message": "chore(release): %s"
        },
        "publish": {
            "assets": []
        }
    },
    "npmClient": "yarn",
    "useNx": false,
    "ignoreChanges": [
        "**/test/**",
        "**/*.md"
    ]
}
